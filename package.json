    "morgan": "~1.7.x",
    "nconf": "~0.8.x",
    "request": "~2.69.x",
    "tv4": "~1.2.x",
    "winston": "~2.2.x",
    "winston-daily-rotate-file": "~1.0.x"
  },
  "devDependencies": {
    "eslint": "^2.11.1",
    "istanbul": "^0.4.3",
    "chai": "~3.5.x",
    "chance": "~1.0.x",
    "eslint": "~2.3.x",
    "jshint": "~2.9.x",
    "jshint-stylish": "~2.0.x",
    "mocha": "~2.4.x",
    "nock": "~8.0.x",
    "rewire": "~2.5.x",
