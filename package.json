  "scripts": {
    "build": "rm -rf dist && rm -rf node_modules && npm i --production && mkdir dist && mkdir dist/project-organizer && cp index.js dist/project-organizer/ && cp package.json dist/project-organizer/ && cp -r node_modules dist/project-organizer/node_modules",
