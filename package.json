{
  "name": "project-organizer",
  "version": "1.0.0",
  "description": "project-organizer",
  "author": "@ThalitaPinheiro",
  "homepage": "https://github.com/ThalitaPinheiro/Curriculo",
  "main": "index.js",
  "scripts": {
    "stryker": "stryker -c stryker.conf.js",
    "build": "rm -rf dist && mkdir dist && mkdir dist/project-organizer && cp index.js dist/project-organizer/index.js && cp docs.js dist/project-organizer/docs.js && cp -r lib dist/project-organizer/lib && cp -r api-docs dist/project-organizer/api-docs && cp -r spec dist/project-organizer/spec && cp -r conf dist/project-organizer/conf && cp package.json dist/project-organizer/package.json && cd dist && npm i --production &&  cd project-organizer/ && mkdir log",
    "check": "./node_modules/.bin/eslint ./**.js utils test;",
    "mocha": "./node_modules/mocha/bin/mocha --recursive",
    "start": "node index.js",
    "debug": "./node_modules/.bin/mocha --recursive --debug-brk",
    "coverage": "istanbul cover --print both _mocha -- -R spec --recursive",
    "pretest": "npm run check",
    "test": "export NODE_ENV=test && npm run coverage",
    "posttest": "./node_modules/.bin/istanbul check-coverage --statements 60 --branches 60 --functions 60"
  },
  "dependencies": {
    "async": "~1.5.x",
    "body-parser": "~1.15.x",
    "express": "~4.13.x",
    "moment": "^2.12.0",
    "mongodb": "~2.1.x",
    "morgan": "~1.7.x",
    "nconf": "~0.11.4",
    "request": "~2.69.x",
    "swagger-framework": "~0.15.0",
    "tv4": "~1.2.x",
    "winston": "~2.2.x",
    "winston-daily-rotate-file": "~1.0.x"
  },
  "devDependencies": {
    "chai": "~3.5.x",
    "chance": "~1.0.x",
    "eslint": "~2.3.x",
    "istanbul": "^0.4.3",
    "jshint": "~2.9.x",
    "jshint-stylish": "~2.0.x",
    "mocha": "~2.4.x",
    "nock": "~8.0.x",
    "rewire": "~2.5.x",
    "stryker": "^0.4.2",
    "stryker-api": "^0.2.0",
    "stryker-html-reporter": "^0.2.0",
    "stryker-mocha-runner": "^0.1.0",
    "supertest": "~1.2.x"
  }
}
